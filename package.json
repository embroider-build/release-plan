    "cli-highlight": "^2.1.11",
    "execa": "^9.5.2",
    "fs-extra": "^11.2.0",
    "github-changelog": "^2.0.0",
    "js-yaml": "^4.1.0",
    "latest-version": "^9.0.0",
    "parse-github-repo-url": "^1.4.1",
    "semver": "^7.3.5",
    "yargs": "^17.0.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.0",
    "@eslint/js": "^9.21.0",
    "@types/fs-extra": "^11.0.4",
