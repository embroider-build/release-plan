    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.2",
    "eslint-plugin-n": "^17.15.1",
    "fixturify-project": "^7.1.3",
    "globals": "^16.0.0",
    "prettier": "^3.5.0",
    "typescript": "^5.2.2",
    "typescript-eslint": "^8.24.1",
    "vitest": "^3.0.6"
  },
